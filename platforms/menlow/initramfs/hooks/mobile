#!/bin/sh

PREREQ=""

prereqs()
{
	echo "$PREREQ"
}

case $1 in
# get pre-requisites
prereqs)
	prereqs
	exit 0
	;;
esac

. /usr/share/initramfs-tools/hook-functions

copy_exec /usr/share/pdk/platforms/mccaslin/initramfs/usb /scripts/usb
copy_exec /usr/share/pdk/platforms/mccaslin/initramfs/disk /scripts/disk
